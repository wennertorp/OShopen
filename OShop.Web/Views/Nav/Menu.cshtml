@model IEnumerable<string>

@Html.ActionLink("Alla produkter","Index","Store", null,
    new {
        @class = "btn btn-block btn-default btn-lg"
    })

@foreach (var category in Model)
{
    @Html.RouteLink(category, new
    {
        controller = "Store",
        action = "Index",
        Category = category
    }, new {
        @class = "btn btn-block btn-default btn-lg" + (category == ViewBag.SelectedCategory ? " btn-primary" : "")
    })
}